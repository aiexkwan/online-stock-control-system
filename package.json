{
  "name": "pennine-stock",
  "version": "0.1.0",
  "private": true,
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "**/polyfills.ts",
    "**/globals.ts",
    "middleware.ts",
    "app/globals.css"
  ],
  "scripts": {
    "dev": "next dev",
    "dev-c": "npm run kill-localhost && npm run clean && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:parallel": "jest --maxWorkers=4",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:unit": "jest --testPathPattern='__tests__/(grn|integration)' --maxWorkers=2",
    "test:integration": "jest --testPathPattern='__tests__/integration' --runInBand",
    "test:performance": "JEST_PERFORMANCE_REPORT=1 npm run test",
    "test:cache-stats": "JEST_CACHE_STATS=1 npm run test",
    "test:profile": "node --inspect-brk node_modules/.bin/jest --runInBand --no-cache",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:pdf": "ts-node __tests__/e2e/run-test.ts",
    "test:sql-injection": "node scripts/test-sql-injection-protection.js",
    "typecheck": "tsc --noEmit",
    "init-auth": "ts-node --project tsconfig.json scripts/init-auth.ts",
    "auto-push": "node scripts/auto-push.js",
    "clean": "rimraf .next .turbo dist build .cache coverage out npm-debug.log",
    "kill-localhost": "node scripts/kill-localhost.js",
    "mcpIOS": "node scripts/set-mcp-config.js && echo 'MCP server configured for Supabase'",
    "analyze": "cross-env ANALYZE=true npm run build",
    "analyze:view": "start bundle-analyzer/client.html",
    "tree-shaking:check": "node scripts/check-tree-shaking.js",
    "tree-shaking:analyze": "npm run tree-shaking:check && npm run analyze",
    "test:perf": "playwright test --config=playwright.performance.config.ts",
    "test:perf:simple": "playwright test e2e/simplified-performance.spec.ts",
    "test:perf:basic": "playwright test e2e/basic-performance.spec.ts --project=chromium",
    "test:perf:check": "node scripts/check-performance.js",
    "test:perf:report": "ts-node tests/performance/run-performance-tests.ts",
    "test:perf:full": "npm run test:perf:report && open test-results/comprehensive-performance-report.html",
    "test:a11y": "playwright test --config=playwright.a11y.config.ts",
    "test:a11y:smoke": "playwright test e2e/a11y/smoke-test.spec.ts --config=playwright.a11y.config.ts",
    "test:a11y:perceivable": "playwright test e2e/a11y/wcag/perceivable.spec.ts --config=playwright.a11y.config.ts",
    "test:a11y:operable": "playwright test e2e/a11y/wcag/operable.spec.ts --config=playwright.a11y.config.ts",
    "test:a11y:report": "playwright show-report playwright-report/a11y",
    "vitest": "vitest",
    "vitest:ui": "vitest --ui",
    "vitest:watch": "vitest --watch",
    "vitest:coverage": "vitest --coverage",
    "vitest:run": "vitest run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "tech-debt:collect": "node scripts/collect-tech-debt-metrics.js",
    "tech-debt:collect:fast": "SKIP_TESTS=true SKIP_BUILD=true node scripts/collect-tech-debt-metrics.js",
    "tech-debt:collect:verbose": "VERBOSE=true node scripts/collect-tech-debt-metrics.js",
    "tech-debt:push": "node scripts/push-tech-debt-metrics.js",
    "tech-debt:collect-and-push": "npm run tech-debt:collect:fast && npm run tech-debt:push",
    "tech-debt:check": "node scripts/apply-tech-debt-thresholds.js check",
    "tech-debt:report": "node scripts/apply-tech-debt-thresholds.js report",
    "tech-debt:check:staging": "TECH_DEBT_ENV=staging npm run tech-debt:check",
    "tech-debt:check:production": "TECH_DEBT_ENV=production npm run tech-debt:check",
    "pre-commit:install": "bash scripts/setup-pre-commit.sh",
    "pre-commit:run": "pre-commit run --all-files",
    "pre-commit:skip": "SKIP=tech-debt-check git commit",
    "scan:todo": "node scripts/todo-scanner.js",
    "scan:todo:watch": "nodemon --watch app --watch lib --watch components --ext ts,tsx --exec 'npm run scan:todo'",
    "test:migration": "bash scripts/run-migration-tests.sh all",
    "test:migration:unit": "bash scripts/run-migration-tests.sh unit",
    "test:migration:integration": "bash scripts/run-migration-tests.sh integration", 
    "test:migration:e2e": "bash scripts/run-migration-tests.sh e2e",
    "test:migration:performance": "bash scripts/run-migration-tests.sh performance",
    "test:migration:a11y": "bash scripts/run-migration-tests.sh accessibility",
    "rollback:migration": "bash scripts/migration-rollback.sh",
    "test:graphql-endpoints": "jest --testPathPattern='graphql' --passWithNoTests",
    "test:widget-loading": "jest --testPathPattern='widget.*loading' --passWithNoTests",
    "test:basic-functionality": "jest --testPathPattern='basic.*functionality' --passWithNoTests",
    "db:setup:test": "echo 'Test DB setup placeholder'",
    "db:cleanup:test": "echo 'Test DB cleanup placeholder'"
  },
