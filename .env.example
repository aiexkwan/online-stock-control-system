# MCP Server Configuration
HOST=0.0.0.0
PORT=8051
TRANSPORT=sse

# OpenAI API Configuration
# SECURITY: Set via environment variables only
OPENAI_API_KEY=your_openai_api_key_placeholder

# LLM for summaries and contextual embeddings
MODEL_CHOICE=gpt-4o-mini

# RAG Strategies
USE_CONTEXTUAL_EMBEDDINGS=true
USE_HYBRID_SEARCH=true
USE_AGENTIC_RAG=true
USE_RERANKING=true

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_URL=https://your-project-ref.supabase.co

# SECURITY: These keys should be set via secure environment variables
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_placeholder
SUPABASE_ANON_KEY=your_supabase_anon_key_placeholder

# SECURITY: Never commit service role key - set via environment variables only
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_placeholder

# SECURITY: Never commit access token
SUPABASE_ACCESS_TOKEN=your_supabase_access_token_placeholder

# Email Service Configuration
# SECURITY: Set via environment variables only
RESEND_API_KEY=your_resend_api_key_placeholder

# Cache Configuration
CACHE_TYPE=apollo
NODE_ENV=development

# Security Mode Configuration
NEXT_PUBLIC_SECURITY_MODE=strict

# Middleware Authentication Configuration
ENABLE_DETAILED_PERMISSION_CHECK=true
ENABLE_ROUTE_PREFETCH=true

# SECURITY: Never commit test credentials
TEST_SYS_LOGIN=test_user_placeholder
TEST_SYS_PASSWORD=test_password_placeholder

SEMAPHORE_LIMIT=1

# GraphQL Configuration
NEXT_PUBLIC_GRAPHQL_ENDPOINT=/api/graphql
NEXT_PUBLIC_GRAPHQL_TYPE=apollo

# Pino Pretty Transport
PINO_PRETTY_TRANSPORT=false